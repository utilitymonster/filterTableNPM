// api_load-test.js

jest.dontMock("../api_load");

var api_load = require("../api_load");

describe('api_load', () => {
	it('recursively pulls api data', () => {
		var url = 'http://swapi.co/api/starships/?page=1';
		var results = api_load(url, 'next');
		console.log(results);
		//expect(api_load(url, 'next').length).toBeGreaterThan(0);
	});});